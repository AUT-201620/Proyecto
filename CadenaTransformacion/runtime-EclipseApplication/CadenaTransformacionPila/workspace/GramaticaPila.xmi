  <serviciosComputo name="pagoSalud">
    <variables name="tarifa" tipoPrimitivo="Flotante">
      <expresion>
        <expresionTerminal real="1.12"/>
      </expresion>
    </variables>
    <condiciones condicionSi="si" asignacionVariable="//@serviciosComputo.0/@variables.0">
      <expresionCondicion>
        <operacion operador="&amp;&amp;">
          <izquierda>
            <nombreCalificado>
              <ides>pensionado</ides>
              <ides>getViveExterior</ides>
            </nombreCalificado>
          </izquierda>
          <derecha>
            <nombreCalificado>
              <ides>pensionado</ides>
              <ides>getNoTieneGrupoFamiliar</ides>
            </nombreCalificado>
          </derecha>
        </operacion>
      </expresionCondicion>
      <expresionAsignacionVariable>
        <expresionTerminal/>
      </expresionAsignacionVariable>
    </condiciones>
    <condiciones condicionSi="siNo" asignacionVariable="//@serviciosComputo.0/@variables.0">
      <expresionCondicion>
        <expresionTerminal>
          <nombreCalificado>
            <ides>pensionado</ides>
            <ides>getCti</ides>
          </nombreCalificado>
        </expresionTerminal>
      </expresionCondicion>
      <expresionAsignacionVariable>
        <expresionTerminal/>
      </expresionAsignacionVariable>
    </condiciones>
    <expresionRetorno>
      <operacion operador="*">
        <izquierda>
          <nombreCalificado>
            <ides>pensionado</ides>
            <ides>getMontoPension</ides>
          </nombreCalificado>
        </izquierda>
        <derecha>
          <nombreCalificado>
            <ides>tarifa</ides>
          </nombreCalificado>
        </derecha>
      </operacion>
    </expresionRetorno>
  </serviciosComputo>
